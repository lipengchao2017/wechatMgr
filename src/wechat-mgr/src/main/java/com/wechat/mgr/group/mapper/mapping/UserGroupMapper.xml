      <if test="usercode != null">
        USERCODE,
      </if>
      <if test="groupcode != null">
        GROUPCODE,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="usercode != null">
        #{usercode,jdbcType=VARCHAR},
      </if>
      <if test="groupcode != null">
        #{groupcode,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
</mapper>