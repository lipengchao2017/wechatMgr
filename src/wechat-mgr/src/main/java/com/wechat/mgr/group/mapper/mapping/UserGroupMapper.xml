  </resultMap>
  <insert id="insert" parameterType="com.wechat.mgr.group.model.UserGroup">
    insert into WECHAT_BASE_T_USERGROUP (USERCODE, GROUPCODE)
    values (#{usercode,jdbcType=VARCHAR}, #{groupcode,jdbcType=VARCHAR})
  </insert>
